{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedisConnection": {
    "ConnectionString": "redis:${REDIS_PORT},password=${REDIS_PASSWORD},ssl=False,abortConnect=False,allowAdmin=True,defaultDatabase=0,connectTimeout=1000,responseTimeout=1000,syncTimeout=1000,writeBuffer=4096,connectRetry=3,connectRetry=1000,keepAlive=180"
  },
  "PostgresConnection": {
    "ConnectionString": "Server=dataretrieval_db;Port=5433;User ID=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};Database=${POSTGRES_DB};Include Error Detail=true"
  },
  "AllowedHosts": "*"
}
